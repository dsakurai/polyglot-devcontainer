{
    "name": "Dev Container",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "workspaceMount": "",
    "remoteUser": "vscode",
    "containerEnv": {
        "HOME": "/home/vscode"
    },
    "runArgs": [
        "--userns=keep-id",
        "--volume=${localWorkspaceFolder}:/workspaces/${localWorkspaceFolderBasename}:Z"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.vscode-dotnet-pack"
            ]
        }
    }
}