#!fsharp

// Use it like
// cube |>>>| [| F; U; f|]

let (|>>>|) state sequence =
    Array.fold (fun accumulated f -> f accumulated) state sequence

#!fsharp

let F state =
  printf "F"
  state
  
let U state =
  printf "U"
  state
 

#!fsharp

0 |> F |> U

#!fsharp

"hello" |> F |> U

